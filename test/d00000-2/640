            function RefreshCheckCode() {
                $("#checkimg").attr("src", "https://passport.pongo.cn/getimg.aspx?small=small&r=" + Math.random());
            }

            function CsdnCheckCallback(data)
            {
                if(data.LoginStatus)
                {
                    Loginsuccess(data.UserName);
                }
            }

            function Loginsuccess(username) {
                //                $("#hidden_div10").hide(); 目前样式似乎不匹配
