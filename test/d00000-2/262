            function csdnlogin() {
                csdn.login(
                    $("#hidden_div10 input[name='UserName']").val()
                    , $("#hidden_div10 input[name='Password']").val()
                    , $("#hidden_div10 input[name='RememberMe']")[0].checked
                    , function (data) {
                        if (data.status) {
                            Loginsuccess(data.data.userName);
                        }
                        else {
                            if (data.error == "该帐户尚未激活。") {
                                window.location.href = "https://passport.csdn.net/account/active?from=" + encodeURIComponent(window.location.href);
                                return;
                            }
                            $("#hidden_div10 input[name='UserName']").siblings("span").html(data.error).show()
                        }
                    });
            }

            $(document).ready(function () {
                //document.domain = "pongo.cn";
                RefreshCheckCode();
                $("#btnLogin").click(function () {
                    var username = $("#hidden_div10 input[name='UserName']").val();
                    var password = $("#hidden_div10 input[name='Password']").val();

                    $("#hidden_div10 input[name='UserName']").siblings("div").hide();
                    $("#hidden_div10 input[name='Password']").siblings("div").hide();

                    if ($.trim(username) == "") {
                        $("#hidden_div10 input[name='UserName']").siblings("span").html("用户名为必填").show();
                        return false;
                    }

                    if ($.trim(password) == "") {
                        $("#hidden_div10 input[name='Password']").siblings("span").html("密码为必填").show();
                        return false;
                    }
                    csdnlogin();
                });

                 $("#usernameid,#pwdid").keydown(function(e){
                    if(e.keyCode == 13){
                        $("#btnLogin").trigger("click");
                    }
                });
            });
        </script>
