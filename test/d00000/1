<script type="text/javascript"> 
$(document).ready(function()
{
  //批量申请职位
  $("#deliver1,#deliver2").click(function()
  {
  		var sltlength=$("#formjobslist input:checked").length;
  		if ($("#formjobslist .list input:checked").length==0)
		{
		dialog("系统提示","text:请选择职位","300px","auto","text");
		}
		else
	 	{
			var jidArr=new Array();
			 $("#formjobslist .list :checkbox[checked]").each(function(index){jidArr[index]=$(this).val();});
			dialog("申请职位","url:get?/user/user_apply_jobs.php?id="+jidArr.join("-")+"&act=app","500","auto","");
	 	}
  });
  
  //全选反选
	$("input[name='selectall']").click(function(){$("#formjobslist :checkbox").attr('checked',$(this).attr('checked'))});
   
 	//单个申请职位
	$(".app_jobs").click(function(){
	dialog("申请职位","url:get?/user/user_apply_jobs.php?id="+$(this).attr("id")+"&act=app","500","auto","");
	});
	//单个添加收藏
	$(".add_favorites").click(function(){
	dialog("加入收藏","url:get?/user/user_favorites_job.php?id="+$(this).attr("id")+"&act=add","500","auto","");
	});
	//批量添加收藏
	  $("#collect1,#collect2").click(function()
	  {
			var sltlength=$("#formjobslist input:checked").length;
			if ($("#formjobslist .list input:checked").length==0)
			{
			dialog("系统提示","text:请选择职位","300px","auto","text");
			}
			else
			{
				var jidArr=new Array();
				 $("#formjobslist .list :checkbox[checked]").each(function(index){jidArr[index]=$(this).val();});
				dialog("加入收藏","url:get?/user/user_favorites_job.php?id="+jidArr.join("-")+"&act=add","500","auto","");
			}
	  });
});
</script>
